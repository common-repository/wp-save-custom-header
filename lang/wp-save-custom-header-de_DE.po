#: wp-save-custom-header.php:326
#, php-format
#@ wp-save-custom-header
msgid "Please make the <code>%s</code> folder of your theme writable!"
msgstr "Bitte mache den <code>%s</code> Ordner in deinem Theme beschreibbar!"

#: wp-save-custom-header.php:675
#@ wp-save-custom-header
msgid "Store header images in this template folder"
msgstr "Kopfzeilen in folgendem Templateordner speichern"

#: wp-save-custom-header.php:703
#@ wp-save-custom-header
msgid "Default is <code>images/headers</code>"
msgstr "Grundeinstellung ist <code>images/headers</code>"

#: wp-save-custom-header.php:729
#@ wp-save-custom-header
msgid "Header images should not be stored in the root directory of your theme. Please specify a folder and try again."
msgstr "Kopfzeilen sollten nicht im Root-Verzeichnis des Themes gespeichert werden. Bitte gib einen anderen Ordner an und versuche es erneut."

#: wp-save-custom-header.php:738
#@ wp-save-custom-header
msgid "The specified folder does not exist! Please create the folder, make it writable and try again."
msgstr "Der angegebene Ordner existiert nicht! Bitte erstelle den Ordner, mache ihn beschreibbar und versuche es erneut."

#: wp-save-custom-header.php:747
#@ wp-save-custom-header
msgid "The specified folder is not writable! Please make it writable and try again."
msgstr "Der angegebene Ordner ist nicht beschreibbar! Bitte mache ihn beschreibbar und versuche es erneut."

